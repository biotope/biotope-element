{
  "name": "@biotope/element",
  "title": "Biotope Element",
  "version": "3.2.0",
  "description": "Biotope Element",
  "main": "lib/esm/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "start": "npm run docs",
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "release": "np",
    "docs": "docsify serve docs",
    "netlify": "mv docs docs-dist",
    "test": "jest",
    "test:report": "npm run test && codecov",
    "test:watch": "jest --watch",
    "lint": "npm run lint:code && npm run lint:tests",
    "lint:code": "biotope-quality-gate lint -p src",
    "lint:code:fix": "biotope-quality-gate lint -p src -f",
    "lint:tests": "biotope-quality-gate lint -p tests -e -c quality-gate-jest.js",
    "lint:tests:fix": "biotope-quality-gate lint -p tests -e -c quality-gate-jest.js -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biotope/biotope-element.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/biotope/biotope-element/issues"
  },
  "homepage": "https://github.com/biotope/biotope-element#readme",
  "dependencies": {
    "dasherize": "^2.0.0",
    "hyperhtml-element": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "~7.4",
    "@biotope/quality-gate": "^1.0.0-beta.5",
    "@types/jest": "~24.0",
    "basichtml": "^0.21.2",
    "basichtml-elements": "^0.1.0",
    "codecov": "^3.5.0",
    "cpy-cli": "~2.0",
    "docsify-cli": "^4.3.0",
    "eslint-plugin-jest": "~22.6.4",
    "jest": "~24.8",
    "np": "~5.0.3",
    "prismjs": "^1.15.0",
    "rollup": "^1.14.4",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-progress": "^1.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-visualizer": "^1.1.0",
    "ts-jest": "~24.0",
    "typescript": "~3.5.1"
  }
}
